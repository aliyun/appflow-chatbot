{
  "name": "@ali/appflow-chat",
  "version": "0.0.1",
  "description": "Appflow-Chat AI聊天机器人组件库，提供聊天服务和UI组件",
  "type": "module",
  "main": "./dist/appflow-chat.cjs.js",
  "module": "./dist/appflow-chat.esm.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/appflow-chat.esm.js",
      "require": "./dist/appflow-chat.cjs.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "publish:ali": "npm run build && node scripts/publish-ali.js",
    "publish:npm": "npm run build && node scripts/publish-npm.js"
  },
  "dependencies": {
    "@ali/chatui-sdk": "^6.6.5",
    "axios": "^1.7.9",
    "js-cookie": "^3.0.5",
    "katex": "^0.16.11",
    "lodash-es": "^4.17.21",
    "react-markdown": "^9.0.1",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "styled-components": "^6.1.13",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "antd": "^5.24.8",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "less": "^4.2.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0"
  },
  "peerDependencies": {
    "antd": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "README-ZH.md"
  ],
  "keywords": [
    "appflow",
    "chat",
    "chatbot",
    "ai",
    "react",
    "components",
    "markdown",
    "streaming"
  ],
  "author": "Appflow Team",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.anpm.alibaba-inc.com",
    "access": "public"
  },
  "overrides": {
    "mdast-util-gfm-autolink-literal": "2.0.0"
  }
}